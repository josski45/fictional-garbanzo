# Telegram Bot Configuration
BOT_TOKEN=your_bot_token_here
WEBHOOK_URL=https://yourdomain.com/webhook.php

# Secret Keys
SECRET_KEY=JSK
DEFAULT_ENCRYPTION_KEY=Match&Ocean

# API Configuration
# NekoLabs API - All-in-one downloader (RECOMMENDED)
NEKOLABS_API_VERSION=v1   # Options: v1, v2, v3, v4

# Ferdev API (DEPRECATED - Use NekoLabs instead)
